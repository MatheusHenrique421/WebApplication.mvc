@model WebApplication.mvc.Models.Inscrito

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete o Inscrito</h1>

<h3>Tem certeza de que deseja excluir este registro?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nome:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            Data Nascimento:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DtNascimento)
        </dd>
        <dt class="col-sm-2">
            E-mail:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            Instagram:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Instagram)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Excluir" class="btn btn-success" />
        <a asp-action="Index" class="btn btn-danger">Voltar</a>
    </form>
</div>
